generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}
        
model User{
  id   Int @id @default (autoincrement())
 nama String
 umur Int
}

